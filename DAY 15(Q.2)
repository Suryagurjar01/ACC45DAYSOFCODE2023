#include <iostream>
using namespace std;

int main() 
{
	int t;
	cin>>t;
	while(t--)
	{
	    int n,hp=0;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    for(int i=0;i<n;i++)
	    {
	        if(hp<arr[i])
	        {
	            hp=arr[i];
	        }
	    }
	    cout<<hp<<endl;
	}
	return 0;
}
